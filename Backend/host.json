{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "http": {
      "cors": {
        "allowedOrigins": [
          "http://localhost:4200",
          "http://127.0.0.1:4200"
        ],
        "supportCredentials": true
      }
    }
  }
}
